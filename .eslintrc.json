{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "script",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": false
        }
    },
    "env": {
        "browser": true,
        "mocha": true,
        "jquery": true
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ]
    }
}
